<form method="post" action="/members/<%= @member.id %>">
  <label for="name-field"> Name:</label>
  <input
  type="text"
  name="name"
  id="name-field"
  value="<%= @member.name %>"
  />

  <label for="age-field"> Age:</label>
  <input
  type="number"
  name="name"
  id="age-field"
  value="<%= @member.age %>"
  />
  <input type="submit" value="Edit Member"
  />
</form>
